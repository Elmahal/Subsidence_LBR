        ee.Feature(
            ee.Geometry.Point([-91.18571403215199, 30.53335409099411]),
            {
              "Class": 5,
              "system:index": "91"
            })]);




//Load the Sentinel-2 SR ImageCollection, filtered by date and ROI.
var dataset = ee.ImageCollection('COPERNICUS/S2_SR_HARMONIZED')
                .filterDate('2022-01-01', '2022-12-31') // Filter to include only images from 2022.
                .filterBounds(LBR_roi)
                .map(function(image) { return image.clip(LBR_roi); }); // Clip each image to the ROI.

// Visualization parameters.
var visParams = {
  bands: ['B4', 'B3', 'B2'],
  min: 0,
  max: 3000,
  gamma: 1.4
};
// Generate a median composite of the filtered collection.
var clippedImage = dataset.median(); // Composite using the median value.
// Center the map on the ROI and add the clipped composite layer.
Map.centerObject(LBR_roi, 10); // Adjust zoom level as necessary.
Map.addLayer(clippedImage, visParams, 'RGB');

var label = 'Class';
var bands = ['B2', 'B3', 'B4', 'B8', 'B11', 'B12']; // Choose bands relevant to your classification

 // Merge all FeatureCollections into one for training
var trainingPoints = water
  .merge(forest)
  .merge(Agric)
  .merge(industrial)
  .merge(urban)
  .merge(roads);
  // Load Sentinel-2 imagery for the same region and time span as your training data
var sentinel2 = ee.ImageCollection('COPERNICUS/S2_SR_HARMONIZED')
  .filterDate('2022-01-01', '2022-12-31')
  .filterBounds(trainingPoints.geometry())
  .filter(ee.Filter.lte('CLOUDY_PIXEL_PERCENTAGE', 20))
  .median();

// Select bands relevant for classification
var bands = ['B2', 'B3', 'B4', 'B8', 'B11', 'B12'];
var input = sentinel2.select(bands);
// Sample the input imagery to create a FeatureCollection of training data
var training = input.sampleRegions({
  collection: trainingPoints,
  properties: ['Class'],
  scale: 10
});
// Train a classifier
var classifier = ee.Classifier.smileRandomForest(50).train({
  features: training,
  classProperty: 'Class',
  inputProperties: bands
});
// Classify the image
var classified = input.classify(classifier);

// Visualize the classification
Map.addLayer(classified, {
  min: 0, 
  max: 5, 
  palette: ['blue', 'green', 'yellow', 'magenta', 'orange', 'red']
}, 'Land Cover Classification');

// Optionally, set the map's view to the training data's extent
Map.centerObject(trainingPoints, 10);
// Example: Hypothetical allPoints FeatureCollection
var allPoints = water.merge(forest).merge(Agric).merge(industrial).merge(urban).merge(roads);

// Randomly split the points into training and validation sets
var withRandom = allPoints.randomColumn('random');

var trainingPoints = withRandom.filter(ee.Filter.lt('random', 0.7));
var validationPoints = withRandom.filter(ee.Filter.gte('random', 0.7));

// Assuming 'input' is your classified image and 'classifier' is already trained
// Classify the validation dataset
var validation = input.sampleRegions({
  collection: validationPoints,
  properties: ['Class'],
  scale: 10
});
var validated = validation.classify(classifier);

// Generate a confusion matrix
var confusionMatrix = validated.errorMatrix('Class', 'classification');
print('confusionMatrix:', confusionMatrix);

// Calculate and print overall accuracy
var overallAccuracy = confusionMatrix.accuracy();
print('Overall Accuracy:', overallAccuracy);

// Calculate and print per-class accuracy
var consumersAccuracy = confusionMatrix.consumersAccuracy(); // User's accuracy
var producersAccuracy = confusionMatrix.producersAccuracy(); // Producer's accuracy
print('Per-Class Consumers Accuracy:', consumersAccuracy);
print('Per-Class Producers Accuracy:', producersAccuracy);

// Define export parameters
var exportParams = {
  image: classified, // The image to export, in this case, the classified image
  description: 'Land_Cover_Classification', // Description of the export task
  scale: 10, // The resolution in meters; adjust according to your needs
  region: trainingPoints.geometry().bounds(), // Define the region to export; here it's the bounds of the training points
  fileFormat: 'GeoTIFF', // Set the file format to GeoTIFF
  folder: 'GEE_Exports', // Specify the Google Drive folder to export to; create it in your Drive if it doesn't exist
  maxPixels: 1e9 // Adjust the maxPixels if your area is large to avoid errors
};

// Start the export task
Export.image.toDrive(exportParams);

